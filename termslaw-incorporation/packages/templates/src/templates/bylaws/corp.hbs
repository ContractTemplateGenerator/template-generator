<article class="document bylaws">
  <header>
    <h1>{{companyName}} Corporate Bylaws</h1>
    <p>State of Incorporation: {{state}}</p>
  </header>

  <section>
    <h2>Article I — Organization</h2>
    <p>The Corporation was formed pursuant to the laws of {{state}} on {{effectiveDate}}.</p>
  </section>

  {{> ownership-table title="Article II — Shareholders" ownerLabel="Shareholder" owners=shareholders unitLabel="Shares" }}

  {{> directors directors=directors }}

  {{> officers officers=officers }}

  {{> distributions governingBody="Board of Directors" ownerLabelPlural="Shareholders" }}

  {{> voting ownerLabelPlural="Shareholders" ownerLabelPluralLower="shareholders" }}

  {{> signatures effectiveDate=effectiveDate signatories=directorNames }}
</article>
