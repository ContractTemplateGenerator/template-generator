<article class="document oa">
  <header>
    <h1>{{companyName}} Operating Agreement</h1>
    <p>State of Formation: {{state}}</p>
  </header>

  <section>
    <h2>Article I — Organization</h2>
    <p>This Operating Agreement (the "Agreement") is entered into effective {{effectiveDate}} by and among the Members listed herein.</p>
  </section>

  {{> ownership-table title="Article II — Members" ownerLabel="Member" owners=members unitLabel="Units" }}

  {{> management-clause isManagerManaged=isManagerManaged managers=managers }}

  {{> distributions governingBody=governingBody ownerLabelPlural="Members" }}

  {{> voting ownerLabelPlural="Members" ownerLabelPluralLower="members" }}

  {{> signatures effectiveDate=effectiveDate signatories=memberNames }}
</article>
