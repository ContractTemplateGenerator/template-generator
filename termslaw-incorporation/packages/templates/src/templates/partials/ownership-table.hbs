<section class="ownership">
  <h3>{{title}}</h3>
  <table class="ownership-table">
    <thead>
      <tr>
        <th scope="col">{{ownerLabel}} Name</th>
        <th scope="col">Percentage</th>
        <th scope="col">Issued {{unitLabel}}</th>
      </tr>
    </thead>
    <tbody>
      {{#each owners}}
      <tr>
        <td>{{default name "[Name]"}}</td>
        <td>{{percent}}%</td>
        <td>{{issued}}</td>
      </tr>
      {{/each}}
    </tbody>
  </table>
</section>
