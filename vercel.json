{
  "version": 2,
  "cleanUrls": true,
  "trailingSlash": true,
  "routes": [
    {
      "src": "^/2025/12/04/dmca-takedown-counter-notice-demand-letters/?$",
      "status": 301,
      "headers": { "Location": "/dmca-takedown-counter-notice-demand-letters/" }
    },
    {
      "src": "^/2025/10/26/delaware-c-corporations-for-non-u-s-residents/?$",
      "status": 301,
      "headers": { "Location": "/delaware-c-corporations-for-non-u-s-residents/" }
    },
    {
      "src": "^/2022/11/19/promissory-note-generator/?$",
      "status": 301,
      "headers": { "Location": "/Templates/promissory-note-generator.html" }
    },
    {
      "src": "^/2025/12/05/self-referral-kickbacks-patient-steering-california/?$",
      "status": 301,
      "headers": { "Location": "/Demand-Letters/Medical/self-referral-kickbacks-patient-steering-california.html" }
    },
    {
      "src": "^/2025/12/04/talent-representation-commission-dispute-demand-letters/?$",
      "status": 301,
      "headers": { "Location": "/Demand-Letters/Influencer-Media/talent-representation-commission-dispute-demand-letters.html" }
    },
    {
      "src": "^/2025/12/04/credit-report-error-demand-letters-fcra/?$",
      "status": 301,
      "headers": { "Location": "/credit-report-error-demand-letters-fcra/" }
    },
    {
      "src": "^/2025/12/04/neobank-fintech-frozen-account-demand-letters/?$",
      "status": 301,
      "headers": { "Location": "/neobank-fintech-frozen-account-demand-letters/" }
    },
    {
      "src": "^/2025/12/04/online-defamation-cease-desist-demand-letters/?$",
      "status": 301,
      "headers": { "Location": "/online-defamation-cease-desist-demand-letters/" }
    },
    {
      "src": "^/\\d{4}/\\d{2}/\\d{2}/%[dD][01].*$",
      "status": 301,
      "headers": { "Location": "/Blog/archive.html" }
    },
    {
      "src": "^/category/dispute-resolution/demand-letters/?(.*)$",
      "status": 301,
      "headers": { "Location": "/Demand-Letters/" }
    },
    {
      "src": "^/category/dispute-resolution/?(.*)$",
      "status": 301,
      "headers": { "Location": "/Demand-Letters/" }
    },
    {
      "src": "^/category/contracts/?(.*)$",
      "status": 301,
      "headers": { "Location": "/Templates/" }
    },
    {
      "src": "^/category/startup/?(.*)$",
      "status": 301,
      "headers": { "Location": "/Templates/" }
    },
    {
      "src": "^/(.*)/[Aa][Mm][Pp]/?$",
      "status": 301,
      "headers": { "Location": "/$1/" }
    },
    {
      "src": "^/\\d{1,3}/$",
      "status": 301,
      "headers": { "Location": "/" }
    },
    {
      "src": "/Users/(.*)",
      "status": 301,
      "headers": { "Location": "/" }
    },
    {
      "src": "/chat-widget/(.*)",
      "dest": "/chat-widget/$1"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    }
  ],
  "headers": [
    {
      "source": "/clients/(.*)",
      "headers": [
        { "key": "X-Robots-Tag", "value": "noindex, nofollow, noarchive" },
        { "key": "Cache-Control", "value": "private, no-cache, no-store, must-revalidate" }
      ]
    }
  ]
}


